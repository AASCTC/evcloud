[Unit]
Description=EVCloud-VNC's web service
After=network.target

[Service]
Type=oneshot
ExecStart=/bin/bash /home/uwsgi/evcloud/00_shell/01run_novnc.sh
ExecReload=/bin/bash /home/uwsgi/evcloud/00_shell/01run_novnc.sh
ExecStop=/bin/bash /home/uwsgi/evcloud/00_shell/02stop_novnc.sh
PrivateTmp=true
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
